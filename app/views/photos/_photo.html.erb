<!-- паршал для отображения фотки одной загруженных на странице события -->
<div class="event-photo buffer-both">
  <%#= link_to image_tag event_thumb(@event)%>
  <% unless photo.photo.url.nil?%>
    <%= link_to photo.photo.url do %>
    <!-- без кавычек не работает, в консоли через pry
    проверил там все окей пути работают, не знаю что сделать -->
    <%#= image_tag(photo.photo.thumb.url)%>
    <%= image_tag(photo.photo.thumb.url)%>
    <% end %>
  <% else %>
   <%= image_tag(asset_pack_path('media/images/event_thumb.jpg')) %>
  <% end %>
  Добавил <%= render partial: 'users/user', object: photo.user %>

    <%= link_to(
          fa_icon('trash'), event_photo_path(photo.event, photo),
          method: :delete, data: { confirm: t('.destroy_confirm') }
        ) %>
</div>
